{
  "dashboard": {
    "id": null,
    "title": "Financial Planning - Business Metrics",
    "tags": ["financial-planning", "business"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Daily Simulations Run",
        "type": "graph",
        "targets": [
          {
            "expr": "increase(financial_simulations_total[1d])",
            "refId": "A",
            "legendFormat": "Total Simulations"
          },
          {
            "expr": "increase(financial_simulations_successful_total[1d])",
            "refId": "B",
            "legendFormat": "Successful"
          },
          {
            "expr": "increase(financial_simulations_failed_total[1d])",
            "refId": "C",
            "legendFormat": "Failed"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Simulation Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(financial_simulations_successful_total[1h]) / rate(financial_simulations_total[1h]) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 90},
                {"color": "green", "value": 95}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "User Goal Creation",
        "type": "graph",
        "targets": [
          {
            "expr": "increase(financial_goals_created_total[1d])",
            "refId": "A",
            "legendFormat": "Goals Created"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Investment Portfolio Updates",
        "type": "graph",
        "targets": [
          {
            "expr": "increase(financial_portfolio_updates_total[1d])",
            "refId": "A",
            "legendFormat": "Portfolio Updates"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "AI Recommendations Generated",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(ai_recommendations_generated_total[5m])",
            "refId": "A",
            "legendFormat": "Recommendations/min"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "Market Data Freshness",
        "type": "stat",
        "targets": [
          {
            "expr": "time() - market_data_last_update_timestamp",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "dtdurations",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 300},
                {"color": "red", "value": 600}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 7,
        "title": "PDF Report Generation",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(financial_pdf_reports_generated_total[5m])",
            "refId": "A",
            "legendFormat": "Reports/min"
          },
          {
            "expr": "histogram_quantile(0.95, rate(financial_pdf_generation_duration_seconds_bucket[5m]))",
            "refId": "B",
            "legendFormat": "95th percentile duration"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 8,
        "title": "Banking Integration Health",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(banking_api_requests_successful_total[5m])",
            "refId": "A",
            "legendFormat": "Successful Requests/min"
          },
          {
            "expr": "rate(banking_api_requests_failed_total[5m])",
            "refId": "B",
            "legendFormat": "Failed Requests/min"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      },
      {
        "id": 9,
        "title": "Voice Interface Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(voice_interface_sessions_total[5m])",
            "refId": "A",
            "legendFormat": "Voice Sessions/min"
          },
          {
            "expr": "histogram_quantile(0.95, rate(voice_response_time_seconds_bucket[5m]))",
            "refId": "B",
            "legendFormat": "95th percentile response time"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32}
      }
    ],
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "refresh": "1m"
  }
}